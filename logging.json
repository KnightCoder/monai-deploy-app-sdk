{
  "version": 1,
  "disable_existing_loggers": false,
  "formatters": {
    "single": {
      "format": "%(message)s"
    }
  },
  "handlers": {
    "console": {
      "class": "logging.StreamHandler",
      "formatter": "single"
    }
  },
  "root": {
    "level": "INFO",
    "propagate": false,
    "handlers": [
      "console"
    ]
  }
}